<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
   <suite name="cutes-tests" domain="system">
       <description>Cutes automatic tests</description>
       <set name="unit-tests" feature="cutes tests">
           <description>Testing cutes</description>
           <case manual="false" name="unittests">
               <step>cd @TESTS_DIR@ &amp;&amp; ./run_tests -v</step>
           </case>
       </set>
   </suite>
</testdefinition>
